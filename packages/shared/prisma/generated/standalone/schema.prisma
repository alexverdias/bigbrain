model ResolvedProcedure {
  id          String    @id @default(uuid()) @db.Char(36)
  procedureId String
  userId      String
  createdAt   DateTime  @default(now())
  finishedAt  DateTime?
}

datasource db {
  provider = "mysql"
  url      = env("STANDALONE_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "../generated/standalone"
}
